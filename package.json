{
  "name": "@coltenkrauter/eslint-config",
  "version": "0.2.0",
  "description": " This package provides eslint configuration as an extensible shared config. ",
  "main": "index.js",
  "scripts": {
    "test-debug": "jest --debug",
    "test": "jest",
    "lint": "eslint src test --config index.js",
    "fix": "eslint src test --config index.js --fix",
    "lint:jest": "eslint --config ./src/jest.js test/fixtures/jestfile.test.js",
    "lint:jest:fix": "eslint --config ./src/jest.js test/fixtures/jestfile.test.js --fix",
    "lint:react-testing-library": "eslint --config ./src/react-testing-library.js test/fixtures/react.test.js",
    "lint:react-testing-library:fix": "eslint --config ./src/react-testing-library.js test/fixtures/react.test.js --fix",
    "lint:react-ts": "eslint --config ./src/react-ts.js test/fixtures/react-file.tsx",
    "lint:react-ts:fix": "eslint --config ./src/react-ts.js test/fixtures/react-file.tsx --fix",
    "lint:react": "eslint --config ./src/react.js test/fixtures/react-file.jsx",
    "lint:react:fix": "eslint --config ./src/react.js test/fixtures/react-file.jsx --fix",
    "lint:typescript": "eslint --config ./src/typescript.js test/fixtures/tsfile.ts",
    "lint:typescript:fix": "eslint --config ./src/typescript.js test/fixtures/tsfile.ts --fix",
    "lint:all": "npm run lint:js && npm run lint:jest && npm run lint:react-testing-library && npm run lint:react-ts && npm run lint:react && npm run lint:typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coltenkrauter/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "krauter",
    "krauter-eslint",
    "krauter-eslint-config",
    "coltenkrauter",
    "coltenkrauter-eslint-config"
  ],
  "author": {
    "name": "Colten Krauter",
    "url": "https://github.com/coltenkrauter"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coltenkrauter/eslint-config/issues"
  },
  "homepage": "https://github.com/coltenkrauter/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "eslint": ">=8",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@types/eslint": "^8.44.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-airbnb": "^0.0.1-security",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-unicorn": "^48.0.1"
  }
}
